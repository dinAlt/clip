{
  "margins:a4": {
    "margin_top": 12,
    "margin_bottom": 12,
    "margin_left": 25,
    "margin_right": 25
  },
  "medium:post": {
    "url_regexp": "medium\\.com",
    "query": "article>div>section>div>div",
    "custom_styles": "body>div{width:auto!important;max-width:none!important;margin:0!important}h1{margin-top:0!important}img[data-tex]{visibility:visible!important}"
  },
  "habr:post": {
    "url_regexp": "habr\\.com",
    "query": "article",
    "remove": ".for_users_only_msg",
    "custom_styles": "img[data-tex]{visibility:visible!important}"
  },
  "habr:comments": {
    "query": "#comments",
    "remove": ".for_users_only_msg",
    "custom_styles": "img[data-tex]{visibility:visible!important}"
  },
  "habr:post_with_comments": {
    "query": "article,#comments",
    "remove": ".for_users_only_msg",
    "custom_styles": "img[data-tex]{visibility:visible!important}"
  },
  "yandex_zen:post": {
    "url_regexp": "zen\\.yandex\\.ru",
    "query": ".article__middle",
    "no_break_after": "h3",
    "no_break_inside": "figure",
    "custom_styles": "img{opacity:1!important}.article__middle{width:auto!important}.article-header{margin-top:0!important}",
    "force_image_loading": true
  },
  "restfulapi:index": {
    "url_regexp": "restfulapi\\.net",
    "query": ".content",
    "remove": ".comment-respond",
    "custom_styles": ".content{width:auto}"
  }
}
